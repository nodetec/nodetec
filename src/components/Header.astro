---
import { SITE } from "../settings";
import Search from "./Search";
import MobileSidebar from "./MobileSidebar";
import MobileMenu from "./MobileMenu";
import ThemeMenu from "./ThemeMenu";
import Lightning from "./Lightning";
import { getCollection } from "astro:content";
import DocNavigation from "./DocNavigation.astro";
import {
  MagnifyingGlassIcon,
  ChevronRightIcon,
} from "@heroicons/react/24/solid";

import type { ContentType, DocType } from "../content/config";

type Props = {
  currentPage: string;
};

const { currentPage } = Astro.props;

// get first part of the path
const collection = currentPage.split("/")[2] as ContentType;

const docs = await getCollection(collection, (item: { id: string }) => {
  return !item.id.endsWith("index.md");
});

function parseMarkdown(doc: any) {
  const markdownContent = doc.body;
  const label = doc.data.label;
  const slug = `guide/${collection}/${doc.slug}`;

  // Split by line
  let lines = markdownContent.split("\n");

  let parsedContent = [];
  let currentHeader = null;

  for (let line of lines) {
    // Check if line is a header
    if (line.startsWith("#")) {
      // Start new header
      currentHeader = line.replace(/#/g, "").trim();
    }

    // Else, it's a paragraph. Add it to parsedContent with the currentHeader
    else if (line.trim().length > 0) {
      if (line.trim().startsWith("```")) {
        continue;
      }
      // ignore empty lines
      parsedContent.push({
        label,
        header: currentHeader,
        paragraph: line.trim(),
        slug,
      });
    }
  }

  return parsedContent;
}

const parsed_markdown = docs.map((doc) => {
  return parseMarkdown(doc);
});

const LANG = "en";

const metaData = await getCollection(collection, (item: { id: string }) => {
  return item.id.endsWith("index.md");
});

let folders = metaData.map((folder: any) => {
  return {
    id: folder.id.split("/").slice(0, -1).join("/"),
    label: folder.data.label,
    slug: `${LANG}/${folder.slug}`,
    order: folder.data.order,
  };
});

folders.sort((a, b) => a.order - b.order);

const files = await getCollection(collection, (item: { id: string }) => {
  return !item.id.endsWith("index.md");
});

// getid by looking up by slug

let currentDocLabel = "";
let currentDocFolder = null;

if (currentPage !== "/") {
  let currentDoc: DocType | undefined = docs.find((doc: DocType) => {
    return `/${doc.slug}/` === currentPage || `/${doc.slug}` === currentPage;
  });

  if (!currentDoc) {
    currentDoc = new Object() as DocType;
  }

  if (currentDoc !== undefined && currentDoc.data !== undefined) {
    currentDocLabel = currentDoc.data.label;
    let currentDocId: any = currentDoc?.id;
    let slashPosition = currentDocId?.lastIndexOf("/");
    if (slashPosition !== -1) {
      currentDocId = currentDocId.substring(0, slashPosition);
    }

    currentDocFolder = metaData.find(function (item: { id: string }) {
      return item.id.startsWith(currentDocId);
    });

    if (!currentDocFolder) {
      currentDocFolder = new Object() as DocType;
    }
  }
}
